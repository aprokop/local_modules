#%Module
proc ModulesHelp { } { }

set _module_name  [module-info name]
set is_module_rm  [module-info mode remove]
set sys           [uname sysname]
set os            [uname release]

set VALGRIND_VERSION    3.10.1
set VALGRIND_PATH       $env(HOME)/local/opt/valgrind-$VALGRIND_VERSION

prepend-path    CPATH               $VALGRIND_PATH/include
prepend-path    LD_LIBRARY_PATH     $VALGRIND_PATH/lib
prepend-path    MANPATH             $VALGRIND_PATH/share/man
prepend-path    PATH                $VALGRIND_PATH/bin
