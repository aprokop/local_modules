#%Module

conflict boost

set _module_name  [module-info name]
set is_module_rm  [module-info mode remove]
set sys           [uname sysname]
set os            [uname release]

set BOOST_VERSION 1.58.0
set BOOST_PATH /home/aprokop/local/opt/boost-$BOOST_VERSION-cxx11

prepend-path    CPATH           $BOOST_PATH/include
prepend-path    LD_LIBRARY_PATH $BOOST_PATH/lib
