#%Module
proc ModulesHelp { } { }

set _module_name  [module-info name]
set is_module_rm  [module-info mode remove]
set sys           [uname sysname]
set os            [uname release]

set EMACS_VERSION 24.4
set EMACS_PATH $env(HOME)/local/opt/emacs-$EMACS_VERSION

prepend-path    PATH            $EMACS_PATH/bin
prepend-path    LD_LIBRARY_PATH $EMACS_PATH/lib
prepend-path    MANPATH         $EMACS_PATH/share/man
prepend-path    INFOPATH        $EMACS_PATH/share/info
