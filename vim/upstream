#%Module
proc ModulesHelp { } { }

set _module_name  [module-info name]
set is_module_rm  [module-info mode remove]
set sys           [uname sysname]
set os            [uname release]

set VIM_PATH       $env(HOME)/local/opt/vim

prepend-path    PATH            $VIM_PATH/bin
prepend-path    MANPATH         $VIM_PATH/share/man
