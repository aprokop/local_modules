#%Module
proc ModulesHelp { } { }

set _module_name  [module-info name]
set is_module_rm  [module-info mode remove]
set sys           [uname sysname]
set os            [uname release]

set AUTOMAKE_VERSION     1.15
set AUTOMAKE_PATH        $env(HOME)/local/opt/automake-$AUTOMAKE_VERSION

prepend-path    PATH            $AUTOMAKE_PATH/bin
prepend-path    MANPATH         $AUTOMAKE_PATH/share/man
