#%Module

proc ModulesHelp { } {
}

set _module_name  [module-info name]
set is_module_rm  [module-info mode remove]
set sys           [uname sysname]
set os            [uname release]

set GDB_PATH $env(HOME)/local/opt/gdb

prepend-path    PATH            $GDB_PATH/bin
prepend-path    CPATH           $GDB_PATH/include
prepend-path    LD_LIBRARY_PATH $GDB_PATH/lib
prepend-path    MANPATH         $GDB_PATH/share/man
prepend-path    INFOPATH        $GDB_PATH/share/info
