#%Module

proc ModulesHelp { } {
}

set _module_name  [module-info name]
set is_module_rm  [module-info mode remove]
set sys           [uname sysname]
set os            [uname release]

set MATIO_VERSION 1.5.2
set MATIO_PATH $env(HOME)/local/opt/matio-$MATIO_VERSION

prepend-path    PATH                $MATIO_PATH/bin
prepend-path    CPATH               $MATIO_PATH/include
prepend-path    LD_LIBRARY_PATH     $MATIO_PATH/lib
prepend-path    MANPATH             $MATIO_PATH/share/man
prepend-path    INFOPATH            $MATIO_PATH/share/info
prepend-path    PKG_CONFIG_PATH     $MATIO_PATH/lib/pkgconfig
