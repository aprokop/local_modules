#%Module
proc ModulesHelp { } { }

set _module_name  [module-info name]
set is_module_rm  [module-info mode remove]
set sys           [uname sysname]
set os            [uname release]

set VIFM_PATH       /home/aprokop/local/opt/vifm

prepend-path    PATH        $VIFM_PATH/bin
prepend-path    MANPATH     $VIFM_PATH/share/man
