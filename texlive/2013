#%Module
proc ModulesHelp { } { }

set _module_name  [module-info name]
set is_module_rm  [module-info mode remove]
set sys           [uname sysname]
set os            [uname release]

set TEXLIVE_VERSION     2013
set TEXLIVE_PATH        $env(HOME)/local/opt/texlive-$TEXLIVE_VERSION

prepend-path    PATH    $TEXLIVE_PATH/bin
