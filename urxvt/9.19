#%Module
proc ModulesHelp { } { }

set _module_name  [module-info name]
set is_module_rm  [module-info mode remove]
set sys           [uname sysname]
set os            [uname release]

set URXVT_VERSION   9.19
set URXVT_PATH      $env(HOME)/local/opt/urxvt-$URXVT_VERSION

prepend-path    PATH                $URXVT_PATH/bin
prepend-path    LD_LIBRARY_PATH     $URXVT_PATH/lib
prepend-path    MANPATH             $URXVT_PATH/share/man
